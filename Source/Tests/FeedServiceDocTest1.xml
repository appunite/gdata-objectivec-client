<?xml version="1.0" encoding="UTF-8"?>
<app:service xmlns:app="http://purl.org/atom/app#">
    <app:workspace title="wilmaflintstone">
        <app:collection
            href="http://photos.googleapis.com/data/feed/api/user/fredflintstone%40gmail.com" title="photo types">
            <app:accept>image/png,image/gif,video/*,image/jpg</app:accept>
        </app:collection>
    </app:workspace>
    <app:workspace title="forgespace">
        <app:collection href="http://example.org/hrefcollection"
          title="Remaindered Links"
          xmlns:atom="http://www.w3.org/2005/Atom">
            <app:accept>entry</app:accept>
            <app:categories fixed="yes" href="hocus-href" scheme="hocus-scheme">
                <atom:category scheme="http://example.org/extra-cats/" term="joke"/>
                <atom:category scheme="http://example.org/extra-cats/" term="serious"/>
            </app:categories>
        </app:collection>
    </app:workspace>
</app:service>
